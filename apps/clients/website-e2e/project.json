{
  "name": "clients-website-e2e",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/clients/website-e2e/src",
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/clients/website-e2e/cypress.config.ts",
        "testingType": "e2e",
        "devServerTarget": "clients-website:serve"
      },
      "configurations": {
        "production": {
          "devServerTarget": "clients-website:serve:production"
        },
        "ci": {
          "devServerTarget": "clients-website:serve-static"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/clients/website-e2e/**/*.{js,ts}"]
      }
    },
    "sonar": {
      "executor": "@koliveira15/nx-sonarqube:scan",
      "options": {
        "name": "clients-website-e2e",
        "hostUrl": "http://localhost:9000",
        "projectKey": "foxminchan_LawKnowledge",
        "skipTargetDefaults": false,
        "branches": false,
        "qualityGate": true,
        "qualityGateTimeout": "300",
        "skipImplicitDeps": false
      }
    }
  },
  "implicitDependencies": ["clients-website"],
  "tags": []
}
